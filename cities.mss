#city {
  text-name:[name];
  text-face-name:"FreeSans Bold";
  text-size: 24;
  text-halo-fill: #FFF;
  text-halo-radius:1;
}

#town {
  text-name:[name];
  text-face-name:"FreeSans Bold";
  text-size: 16;
  text-halo-fill: #FFF;
  text-halo-radius:1;
}

#village [zoom > 9] {
  text-name:[name];
  text-face-name:"FreeSans Bold";
  text-size: 10;
  text-halo-fill: #FFF;
  text-halo-radius:1;
}

